{
  "majestic": {
    "args": [
      "-i -c ./test/jest-unit.json"
    ],
    "env": {
      "CI": "true"
    }
  },
  "name": "push-notificator",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "copy:shared:modules": "cp -r \"./shared/node_modules\", \"./lib/shared/\"",
    "build": "rimraf dist && tsc --build",
    "build-prod": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "npm run start:dev",
    "build:live": "nodemon --watch src/**/*.ts --exec ts-node src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest -i -c ./test/jest-unit.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "majestic": "majestic --app",
    "start:tunnel": "lt --port 3000 --subdomain orgbotdomain"
  },
  "dependencies": {
    "@nestjs/common": "^6.6.7",
    "@nestjs/core": "^6.6.7",
    "@nestjs/cqrs": "^6.1.0",
    "@nestjs/jwt": "^10.0.1",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.6.7",
    "@nestjs/swagger": "^3.1.0",
    "@nestjs/typeorm": "^6.1.3",
    "@slack/types": "^1.2.1",
    "@slack/web-api": "^5.2.1",
    "@speee-js/jsx-slack": "^0.10.2",
    "@types/amqplib": "^0.5.13",
    "botbuilder-adapter-slack": "^1.0.14",
    "botkit": "^4.15.0",
    "class-transformer": "^0.2.3",
    "class-validator": "0.9.1",
    "clickhouse": "^1.2.21",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "envalid": "^5.0.0",
    "express": "^4.17.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "nestjs-config": "^1.4.3",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.1",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.12",
    "slackify-markdown": "^1.1.1",
    "sqlite3": "^4.1.0",
    "swagger-ui-express": "^4.0.7",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@nestjs/testing": "6.1.1",
    "@types/express": "4.16.1",
    "@types/jest": "^24.0.11",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "jest": "24.7.1",
    "localtunnel": "^2.0.0",
    "nodemon": "^1.19.1",
    "prettier": "1.18.2",
    "sinon": "^7.4.1",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "^3.6.4"
  }
}
